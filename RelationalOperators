#include <iostream>
using namespace std;

int main() {
    int a, b;
    a = 4;
    b = 5;
    bool result;

    result = (a == b);   // false
    cout << "4 == 5 is ( 4 is equal to 5) result is " << result << endl;

    result = (a != b);  // true
    cout << "4 != 5 is  (4 is not equal to 5) result is " << result << endl;

    result = a > b;   // false
    cout << "4 > 5 is  (4 is greater than 5) result is " << result << endl;

    result = a < b;   // true
    cout << "4 < 5 is (4 is less than 5) result is " << result << endl;

    result = a >= b;  // false
    cout << "4 >= 5 is  (4 is greater than or equal to 5) result is " << result << endl;

    result = a <= b;  // true
    cout << "4 <= 5 is  (4 is less than or equal to 5) result is " << result << endl;

    return 0;
}
